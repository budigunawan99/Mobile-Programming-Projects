<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="name_style">
        <item name="android:fontFamily">@font/atomic_age</item>
        <item name="android:textSize">@dimen/name_size</item>
        <item name="android:layout_marginTop">@dimen/name_margin</item>
        <item name="android:paddingStart">@dimen/paddingleft</item>
        <item name="android:paddingEnd">@dimen/paddingright</item>
    </style>

</resources>